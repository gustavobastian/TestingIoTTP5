									"    },",
									"    body:JSON.stringify([\"Juan\",\"Pedro\"])",
									"}),",
									"function(err,response){   ",
									"    console.log(response)",
									"}"
								],
								"type": "text/javascript"
							}
						},
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test(\"test primer movimiento correcto\", function () {",
									"    pm.response.to.have.status(200);     ",
									"    pm.response.to.not.be.error;",
									"    pm.response.to.be.withBody;",
									"    pm.response.to.be.json;    ",
									"    pm.expect(pm.response.json().estado).not.eql(\"No Auth\");",
									"    pm.expect(pm.response.json().estado.length).eql(3);",
									"    pm.expect(pm.response.json().estado[1].length).eql(3);   ",
									"    pm.expect(pm.response.json().estado).eql(",
									"        [",
									"        [\"x\", \" \", \" \" ],",
									"        [\" \", \" \", \" \" ],",
									"        [\" \", \" \", \" \" ]",
									"    ]",
									"",
									"    );   ",
									"     ",
									"});",
									""
								],
								"type": "text/javascript"
							}
						}
					],
